        <CellType class="biological" name="cell2">
            <VolumeConstraint target="500" strength=".1"/>
            <SurfaceConstraint target="1" mode="aspherity" strength=".5"/>
            <ConnectivityConstraint/>
            <MembraneProperty symbol="u_2" value="0.5" name="Activator Concentration - Otsuji">
                <Diffusion rate="0.1"/>
            </MembraneProperty>
            <MembraneProperty symbol="v_2" value="0.5">
                <Diffusion rate="10"/>
            </MembraneProperty>
            <MembraneProperty symbol="s_2" value="0.0">
                <Diffusion rate="0"/>
            </MembraneProperty>
            <System solver="runge-kutta" time-step="0.1" name="Otsuji model">
                <Constant symbol="a1" value="25"/>
                <Constant symbol="a2" value="0.7"/>
                <!--    <Disabled>
        <Constant symbol="u_v_concentration" value="u_2 + v_2"/>
    </Disabled>
-->
                <Constant symbol="s_OT" value="1.0"/>
                <Intermediate symbol="F_2" value="a1*(v_2 + s_2 + (u_2 + v_2)/(a2*s_OT*(u_2 + v_2)+1)^2)"/>
                <DiffEqn symbol-ref="u_2">
                    <Expression>F_2</Expression>
                </DiffEqn>
                <DiffEqn symbol-ref="v_2">
                    <Expression>-F_2</Expression>
                </DiffEqn>
            </System>
            <Mapper name="copy c(x,y,z) to s_2(x,y,z)">
                <Input value="c"/>
                <Output symbol-ref="s_2"/>
            </Mapper>
            <PropertyVector symbol="move_dir_2" value="0.0, 0.0, 0.0"/>
            <Mapper>
                <Input value="u_2"/>
                <Polarity symbol-ref="move_dir_2"/>
            </Mapper>
            <DirectedMotion direction="move_dir_2" strength="0.01"/>
        </CellType>

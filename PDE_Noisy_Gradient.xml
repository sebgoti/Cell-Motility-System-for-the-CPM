-->
        <Field symbol="c" value="0.01 * rand_uni(0,1)" name="Gradient Field">
            <Diffusion rate="0"/>
        </Field>
        <!--    <Disabled>
        <Event trigger="on change">
            <Condition>t > 800</Condition>
